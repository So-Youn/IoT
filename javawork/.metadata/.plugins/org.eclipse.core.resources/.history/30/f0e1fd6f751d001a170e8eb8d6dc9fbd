package oop.Chap07.poly;

public class Coffee extends Beverage{
	private int amount;
	
	public Coffee() {
		
	}
	public Coffee(String name,int amount) {
		super(name);
		this.amount = amount;
	}
	public int getAmount() {
		return amount;
	}

	public void setAmount(int amount) {
		this.amount = amount;
	}
	@Override
	public void calcPrice() {
		
	}
	
}
