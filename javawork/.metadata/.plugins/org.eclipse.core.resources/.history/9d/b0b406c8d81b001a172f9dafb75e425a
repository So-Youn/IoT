package oop.Chap06.constructor;
//메소드의 호출 과정
public class MethodCallExam {
	public void test() {
		first();//test에서 first호출
	}
	public void first() {
		second();
	}
	public void second() {
		System.out.println("second()~~~~");
	}
	
	public static void main(String[] args) {
		MethodCallExam obj = new MethodCallExam();
		obj.test();
	}
}
