package oop.Chap07.poly;

public class Tea extends Beverage{
private int amount;
	
	public Tea() {
		
	}
	public Tea(String name,int amount) {
		super(name);
		this.amount = amount;
	}
	public int getAmount() {
		return amount;
	}

	public void setAmount(int amount) {
		this.amount = amount;
	}
	@Override
	public void calcPrice(int price) {
		
	}

}
