package Test;

import java.util.Scanner;

public class Main {
	// ctrl+shif+f : ÁÙ Á¤¸®
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		int H = input.nextInt();
		int M = input.nextInt();
		input.close();
		if (M >= 45) {
			M = M - 45;
			System.out.println(H + " " + M);
		} else {
			if (H == 0) {
				H = 24;
			}

			H = H - 1;
			M = M - 45 + 60;
			System.out.println(H + " " + M);
		}

	}
}
