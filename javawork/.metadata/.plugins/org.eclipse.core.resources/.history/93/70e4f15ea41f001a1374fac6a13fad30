package oop.Chap07.poly;
/*
 * 객체의 타입을 검사하고 작업할 수 있다.
 * InstanceOf연산자 이용
 */
class A{
	
}
interface IA{
	
}
public class InstanceOfTest {
	public static void main(String[] args) {
		SubA obj = new SubA();
		//obj가 SuperA타입이라면 if블럭을 실행해라,
		//	----------------
		//		ㄴ
		if(obj Instanceof SuperA) {
			System.out.println("SuperA 타입이다.");
		}
	}

}
