package Chap05;

public class Test06 {
	public static void main(String[] args) {
		int sum = 0;
		int[] input= new int[1]; 
		input[0] = 5;
		for(int i = 0; i<=100;i++) {
			if(i%input==0) {
				sum = sum +i;
				System.out.print(sum+"+");
		}else {
			break;
		}
		
			
		}
		System.out.println("="+sum);
	}

}
