package oop.Chap07;
/*
 * <상속관계에서 메소드가 갖는 특징>
 * 
 */
class Parent{
	public void display() {
		System.out.println("부모의 display...");
	}
}
class Child extends Parent{
	public void test() {
		System.out.println("자식 클래스의 test...");
		display();
	}
}
public class InheritanceTest02 {

	public static void main(String[] args) {
		Child obj = new Child();
		obj.test();
	}

}
