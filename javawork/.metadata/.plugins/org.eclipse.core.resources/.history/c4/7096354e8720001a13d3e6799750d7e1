package chap08;

public class CalcTest {
	public static void main(String[] args) {
		int a = Integer.parseInt(args[0]);
		int b = Integer.parseInt(args[2]);
		Calculator calc = new Calculator();
		
		if (args[1].equals("+")) {
			calc.plus(a,b);
		}else if(args[1].equals("-")) {
			calc.minus(a,b);
		}else if(args[1].equals("*")) {
			calc.multiple(a,b);
		}else if(args[1].equals("/")) {
			calc.divide(a,b);
		}
		calc.print();
		
	}

	

}
