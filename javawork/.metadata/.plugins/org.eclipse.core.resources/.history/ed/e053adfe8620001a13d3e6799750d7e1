package chap08;

public class CalcTest {
	public static void main(String[] args) {
		int a = Integer.parseInt(args[0]);
		int b = Integer.parseInt(args[1]);
		Calculator calc = new Calculator();
		
		if (args[2].equals("+")) {
			calc.plus(a,b);
		}else if(args[2].equals("-")) {
			calc.minus(a,b);
		}else if(args[2].equals("*")) {
			calc.multiple(a,b);
		}else if(args[2].equals("/")) {
			calc.divide(a,b);
		}
		calc.print();
		
	}

	

}
