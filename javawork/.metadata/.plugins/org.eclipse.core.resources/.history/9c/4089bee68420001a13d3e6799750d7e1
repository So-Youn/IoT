package chap08;

public class Calculator {
	public static double plus(int a, int b) {
		double result;
		result = a+b;
		return result;
	}
	public static double minus(int a, int b) {
		double result;
		result = a-b;
		return result;
	}
	public static double multiple(int a, int b) {
		double result;
		result = a*b;
		return result;
		
	}
	public static double divide(int a,int b) {
		double result;
		result = a/b;
		return result;
		
	}
}
