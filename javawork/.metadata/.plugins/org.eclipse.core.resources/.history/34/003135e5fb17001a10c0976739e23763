package Chap05;
//배열의 문법 연습 - 사용하는 방법
public class ArrayTest01 {
	public static void main(String[] args) {
		//1. 배열의 선언
		int[] myarr;
		int[] myarr2;
		//2. 배열의 생성
		myarr = new int[5];
		myarr = new int[5];
		System.out.println(myarr);//[I@7852e922; heap에 할당된 배열이 어디에 있는지 주소값을 나타내줌.
		System.out.println(myarr2);
		

	}

}
