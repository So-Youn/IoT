package workbook.ClassTest;

import oop.Chap06.Student;

public class StudentTest {
	public static void main(String[] args) {
		double avg = 0.0;
		Student[] studentArr = new Student[3]; 
		studentArr[0] = new Student("홍길동",15,171,81);
		studentArr[1] = new Student("한사람",13,183,72);
		studentArr[2] = new Student("임걱정",16,175,65);
		
		System.out.println("이름 \t 나이:\t 신장:\t 몸무게:");
		studentArr[0].print();
		studentArr[1].print();
		studentArr[2].print();
		avg = studentArr[0].getAge()+studentArr[1].getAge()+studentArr[2].getAge()/studentArr.length;
		System.out.println("나이의 평균 : "+avg);
	}

}
