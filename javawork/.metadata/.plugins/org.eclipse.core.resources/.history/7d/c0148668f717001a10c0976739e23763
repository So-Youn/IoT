package worktest;

//Break문 연습 - break문이 선언된 블럭을 빠져나간다.
public class GuGu_Break {

	public static void main(String[] args) {
		for (int dan = 1; dan <= 9; dan++) {
			for (int i = 1; i <= 9; i++) {
				if (dan == 5) {
					break;
				}
				System.out.print(dan + "*" + i + "=" + dan * i + "\t");

			}
			System.out.println();
		}
	}

}
