package workbook.ClassTest;

import oop.Chap06.Student;

public class StudentTest {
	public static void main(String[] args) {
		Student str = new Student();
		Student[] studentArr = new Student[3]; 
		studentArr[0] = new str.Student("홍길동",15,171,81);
		studentArr[1] = new str.Student("한사람",13,183,72);
		studentArr[2] = new str.Student("임걱정",16,175,65);
		
		System.out.println("name= +\t 나이:\t);
		System.out.println("studentArr[0].getName()");
	}

}
