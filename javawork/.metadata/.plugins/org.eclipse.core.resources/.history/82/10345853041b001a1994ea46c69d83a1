package oop.Chap06;
//메소드 정의하는 연습을 위한 클래스
public class MyMethod {
	//2. 매개변수가 있고 리턴값이 하나 있는 메소드
	public void display(String str) { 
		for (int i = 1;i<=10;i++) {
			System.out.print(str);
		}
		System.out.println( );
	}
	//1. 매개변수가 없고 리턴값이 없는 메소드
	public void display() { 
		for (int i = 1;i<=10;i++) {
			System.out.print("*");
		}
		System.out.println( );
	}

}
