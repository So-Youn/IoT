package oop.Chap07;

public class Person {
	String name;			//부모클래스는 자식 클래스의 중복된 클래스를 일반화시키기 위함. ->private default
	int age;
	public Person() {
		
	}
	
	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getAge() {
		return age;
	}

	public void setAge(int age) {
		this.age = age;
	}

}
