package Test;

import java.util.Scanner;

public class Main02 {
	public static void main(String[] args) {
		Scanner input =new Scanner(System.in);
		int N = input.nextInt();
		for(int i=1;i<=N;i++) {
			if{
			for (int j =1; j <=i; j++) {
				System.out.print("*");
			}else {
				System.out.println(" ");
			}
			System.out.println( );
			input.close();
		}
	}

}
