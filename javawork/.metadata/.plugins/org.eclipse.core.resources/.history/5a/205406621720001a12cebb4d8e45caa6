package workbook.poly.Animal;

public abstract class Animal {
	private int speed;
	double distance;
	public Animal() {
		
	}
	public Animal(int speed) {
		super();
		this.speed=speed;
		
	}
	public int getSpeed() {
		return speed;
	}
	public void setSpeed(int speed) {
		this.speed = speed;
	}
	
	public abstract void run(int hours);
	
	
}
