package Test;

import java.util.Scanner;

public class Main02 {

	public static void main(String[] args) {
		int A=0;
		int B=0;
		Scanner input = new Scanner(System.in);
		int T = input.nextInt();
		for(int i=0;i<T;i++) {
			A = input.nextInt();
			B = input.nextInt();
			System.out.println(A+B);
		}
		input.close();
		
	}

}
